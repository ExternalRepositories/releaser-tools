{
  "author": {
    "name": "Steve Mao",
    "email": "maochenyan@gmail.com",
    "url": "https://github.com/stevemao"
  },
  "bin": "./cli.js",
  "bugs": "https://gitlab.com/hutson/conventional-gitlab-releaser/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "Make a new GitLab release from git metadata.",
  "engines": {
    "node": "^4.1.0 || ^0.12.0",
    "npm": "^3.0.0 || ^2.1.9"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "homepage": "https://gitlab.com/hutson/conventional-gitlab-releaser",
  "keywords": [
    "conventional-gitlab-releaser",
    "gitlab",
    "release",
    "releases",
    "conventional-changelog",
    "conventional",
    "changelog",
    "log"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "conventional-gitlab-releaser",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/hutson/conventional-gitlab-releaser.git"
  },
  "scripts": {
    "coverage": "istanbul cover _mocha -- -R spec --timeout 50000 && rm -rf ./coverage",
    "lint": "jshint *.js test --exclude node_modules && jscs *.js test",
    "test": "npm run lint"
  },
  "version": "1.0.0",
  "dependencies": {
    "conventional-changelog": "^1.1.0",
    "dateformat": "^1.0.11",
    "git-semver-tags": "^1.0.0",
