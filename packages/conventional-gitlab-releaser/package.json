    "through2": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "concat-stream": "^1.5.1",
    "coveralls": "^2.11.2",
    "cz-conventional-changelog": "^1.1.6",
    "github-remove-all-releases": "^1.0.0",
    "istanbul": "^0.4.1",
    "jscs": "^3.0.7",
    "jshint": "^2.9.1",
    "mocha": "*",
    "semantic-release-gitlab": "^2.0.2",
    "shelljs": "^0.7.4"
  }
}
